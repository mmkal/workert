{
  "name": "workert",
  "version": "0.0.0",
  "description": "POC of TypeScript compiler running in a Cloudflare Worker",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "dev": "vite dev",
    "build": "bun build.ts && vite build",
    "preview": "npm run build && vite preview",
    "deploy": "npm run build && wrangler deploy",
    ".build": "bun run build.ts",
    ".dev": "bun run build && bun run alchemy dev",
    ".deploy": "bun run build && bun run alchemy deploy",
    "destroy": "bun run alchemy destroy"
  },
  "keywords": ["cloudflare", "worker", "typescript", "compiler"],
  "author": "mmakl",
  "license": "Apache-2.0",
  "dependencies": {
    "@ts-morph/bootstrap": "^0.28.1",
    "alchemy": "^0.81.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.18.0",
    "@types/bun": "1.3.4",
    "vite": "7.3.0"
  }
}
